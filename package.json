{
  "name": "crlife-daily",
  "version": "1.0.0",
  "description": "Crossroads Church Daily Bible Study",
  "main": "dist/main.js",
  "scripts": {
    "build:watch": "tsc -w",
    "prestart": "tsc",
    "start": "node ./dist/main.js",
    "dev": "cross-env DEBUG=app* cross-env PORT=3000 nodemon --ignore './data/dev/' ./dist/main.js",
    "dev:azure": "cross-env DEBUG=app* cross-env PORT=3000 cross-env _DAILY_AZURE=true nodemon --ignore './data/dev/' ./dist/main.js",
    "packclient": "node ./util/packclient.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bspain/crlife-dailybibleverse.git"
  },
  "keywords": [
    "CRLife",
    "Daily",
    "Bible"
  ],
  "author": "Ben Spain",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bspain/crlife-dailybibleverse/issues"
  },
  "homepage": "https://github.com/bspain/crlife-dailybibleverse#readme",
  "dependencies": {
    "@azure/storage-blob": "^10.3.0",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.16.4",
    "node-fetch": "^2.6.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/debug": "^4.1.4",
    "@types/express": "^4.16.1",
    "@types/node": "^10.14.8",
    "@types/node-fetch": "^2.3.4",
    "@types/request": "^2.48.1",
    "cross-env": "^5.2.0",
    "ncp": "^2.0.0",
    "nodemon": "^1.19.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.5"
  }
}
